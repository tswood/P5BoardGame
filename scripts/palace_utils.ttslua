Palace = {}

function Palace:revealShadows(quantity)
  local palace_cover = getObjectFromGUID(PALACE_COVER_GUID)
  local cover_pos = palace_cover.getPosition()
  local shadows = Zone:getDeckOrCard(shadow_cards_zone)

  for i=1,quantity do
    shadows.takeObject({
      position = palace_cover + PALACE_COVER_SHADOW_DROP_OFFSET
    })
  end
end
